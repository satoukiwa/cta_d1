<div class="form-group">
          <%= f.label :arrt, "Arrt" %>
          <div class="row">
            <div class='col-md-6 mb-1'>
              <div class='input-group date' id='arrtgteqPicker'>
                  <%= f.text_field :arrt_gteq, :class => "form-control", :placeholder => "Arrt greater than or equal to" %>
                  <span class="input-group-addon">
                      <span class="glyphicon glyphicon-calendar"></span>
                  </span>
              </div>
            </div>

            <div class='col-md-6'>
              <div class='input-group date' id='arrtlteqPicker'>
                  <%= f.text_field :arrt_lteq, :class => "form-control", :placeholder => "Arrt less than or equal to" %>
                  <span class="input-group-addon">
                      <span class="glyphicon glyphicon-calendar"></span>
                  </span>
              </div>
            </div>
          </div>
      </div>
      <script type="text/javascript">
          $(function () {
              $('#arrtgteqPicker').datetimepicker({
                  format: 'YYYY-MM-DD hh:mm:SS'
              });
              $('#arrtlteqPicker').datetimepicker({
                  useCurrent: false,
                  format: 'YYYY-MM-DD hh:mm:SS'
              });
              $('#arrtgteqPicker').on("dp.change", function (e) {
                  $('#arrtlteqPicker').data("DateTimePicker").minDate(e.date);
              });
              $('#arrtlteqPicker').on("dp.change", function (e) {
                  $('#arrtgteqPicker').data("DateTimePicker").maxDate(e.date);
              });
          });
      </script>

